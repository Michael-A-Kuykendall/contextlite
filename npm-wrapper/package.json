{
  "name": "contextlite",
  "version": "1.1.16",
  "description": "Ultra-fast context engine with enterprise workspace clustering for AI applications",
  "keywords": [
    "context",
    "ai",
    "retrieval",
    "search",
    "embedding",
    "database",
    "sqlite",
    "fast",
    "cli",
    "binary",
    "clustering",
    "workspace",
    "enterprise",
    "multi-project"
  ],
  "author": {
    "name": "ContextLite Team",
    "email": "support@contextlite.com",
    "url": "https://contextlite.com"
  },
  "license": "MIT",
  "homepage": "https://contextlite.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Michael-A-Kuykendall/contextlite.git",
    "directory": "npm-wrapper"
  },
  "bugs": {
    "url": "https://github.com/Michael-A-Kuykendall/contextlite/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "contextlite": "bin/contextlite.js"
  },
  "files": [
    "bin/",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "postinstall": "node scripts/fetch-binary.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.0",
    "tar": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/tar": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "preferGlobal": true,
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
